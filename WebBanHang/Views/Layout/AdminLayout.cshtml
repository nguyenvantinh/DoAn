<body>
    <section id="container">
        <!--header start-->
        <header class="header fixed-top clearfix">
            <!--logo start-->
            <div class="brand">
                <a href="index.html" class="logo" >
                    SMART STORE
                </a>
                <div class="sidebar-toggle-box">
                    <div class="fa fa-bars"></div>
                </div>
            </div>
            <!--logo end-->
            <div class="top-nav clearfix">
                <!--search & user info start-->
                <ul class="nav pull-right top-menu">
                    <li>
                        <input type="text" class="form-control search" placeholder=" Search">
                    </li>
                    <!-- user login dropdown start-->
                    <li class="dropdown">
                        @if (Session["NguoiDung"] != null && Session["NguoiDung"].ToString() != "")
                        {
                            <a data-toggle="dropdown" class="dropdown-toggle" href="#">

                                @{
                                    WebBanHang.Models.NguoiDung nd = (WebBanHang.Models.NguoiDung)Session["NguoiDung"];
                                }
                                <img alt="avatar" src="~/Content/AdminLayout/images/1.png">
                                <span class="username" style="text-transform: uppercase;">@nd.TaiKhoan</span>
                                <b class="caret"></b>
                            </a>
                        }

                        <ul class="dropdown-menu extended logout">
                            <li><a href="#"><i class=" fa fa-suitcase"></i>Profile</a></li>
                            <li><a href="#"><i class="fa fa-cog"></i> Settings</a></li>
                            <li><a href="/Admin/LogOut"><i class="fa fa-key"></i> Log Out</a></li>
                        </ul>
                    </li>
                    <!-- user login dropdown end -->

                </ul>
                <!--search & user info end-->
            </div>
        </header>
        <!--header end-->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse">
                <!-- sidebar menu start-->
                <div class="leftside-navigation">
                    <ul class="sidebar-menu" id="nav-accordion">
                        <li>
                            @*<a class="active" href="/Admin/ThongKe">*@
                            <a class="" href="/Admin/ThongKe">
                                <i class="fa fa-dashboard"></i>
                                <span>Dashboard</span>
                            </a>
                        </li>

                        <li class="sub-menu" id="qlsp">
                            <a href="#">
                                <i class="fa fa-book"></i>
                                <span>Quản lý sản phẩm</span>
                            </a>
                            <ul class="sub">
                                <li><a href="/QuanLySanPham">Danh sách sản phẩm</a></li>
                                <li><a href="/QuanLySanPham/SanPhamNoiBat">Sản phẩm nổi bật</a></li>
                                <li><a href="/QuanLySanPham/SanPhamMoi">Sản phẩm mới</a></li>
                                <li><a href="/QuanLySanPham/SanPhamDaXoa">Sản phẩm đã xóa</a></li>
                            </ul>
                        </li>
                        <li>
                            <a href="/QuanLyLoaiSanPham">
                                <i class="fa fa-th"></i>
                                <span>Loại sản phẩm </span>
                            </a>
                        </li>
                        <li class="sub-menu">
                            <a href="/QuanLyNhaSanXuat">
                                <i class="fa fa-industry"></i>
                                <span>Nhà sản xuất</span>
                            </a>
                        </li>
                        <li class="sub-menu">
                            <a href="/QuanLyNhaCungCap">
                                <i class="fa fa-home" aria-hidden="true"></i>
                                <span>Nhà cung cấp</span>
                            </a>
                        </li>
                        <li class="sub-menu">
                            <a href="/QuanLyKhachHang/Index">
                                <i class="fa fa-users" aria-hidden="true"></i>
                                <span>Khách hàng</span>
                            </a>
                        </li>
                        <li class="sub-menu">
                            <a href="/QuanLyDonHang/ChoDuyet">
                                <i class="fa fa-book"></i>
                                <span>Đơn hàng</span>
                            </a>
                        </li>
                        <li class="sub-menu">
                            <a href="/QuanLyPhieuNhap/NhapHang">
                                <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                                <span>Nhập hàng</span>
                            </a>
                        </li>
                        <li class="sub-menu">
                            <a href="/QuanLyPhieuNhap/DSSPHetHang">
                                <i class="fa fa-database" aria-hidden="true"></i>
                                <span>Sản phẩm sắp hết</span>
                            </a>
                        </li>
                        <li class="sub-menu">
                            <a href="/QuanLyQuyen">
                                <i class="fa fa-lock" style="font-size: 20px;"></i>
                                <span> Quản lý quyền </span>
                            </a>
                        </li>
                        <li class="sub-menu">
                            <a href="/QuanLyNguoiDung">
                                <i class="fa fa-user"></i>
                                <span>Người dùng</span>
                            </a>
                        </li>
                    </ul>
                </div>
                <!-- sidebar menu end-->
            </div>
        </aside>
        <!--sidebar end-->
        <!--main content start-->
        <section id="main-content">
            <section class="wrapper">
                @RenderBody()
            </section>
            
        </section>
        <!--main content end-->
    </section>

    <script src="~/Content/AdminLayout/js/jquery-ui.min.js"></script>
    <script src="~/Content/AdminLayout/js/bootstrap.js"></script>

    <script src="~/Content/AdminLayout/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/Content/AdminLayout/js/scripts.js"></script>
    <script src="~/Content/AdminLayout/js/jquery.slimscroll.js"></script>
    <script src="~/Content/AdminLayout/js/jquery.nicescroll.js"></script>
    <!--[if lte IE 8]><script language="javascript" type="text/javascript" src="js/flot-chart/excanvas.min.js"></script><![endif]-->
    <script src="~/Content/AdminLayout/js/jquery.scrollTo.js"></script>
    <!-- morris JavaScript -->
    <script>
        $(document).ready(function () {
            //BOX BUTTON SHOW AND CLOSE
            jQuery('.small-graph-box').hover(function () {
                jQuery(this).find('.box-button').fadeIn('fast');
            }, function () {
                jQuery(this).find('.box-button').fadeOut('fast');
            });
            jQuery('.small-graph-box .box-close').click(function () {
                jQuery(this).closest('.small-graph-box').fadeOut(200);
                return false;
            });
        });
    </script>
    
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/notify.min.js"></script>
